<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="ITU.RefereeAssistant.Domain.Models" assembly="ITU.RefereeAssistant.Domain">

  <class name="Match" table="Match">

    <id name="Id">
      <generator class="native" />
    </id>

    <property name="MatchResult" />

    <many-to-one name="FirstPlayer" class="Player" />

    <many-to-one name="SecondPlayer" class="Player" />

    <many-to-one name="Round" class="Round" />

  </class>

</hibernate-mapping>
